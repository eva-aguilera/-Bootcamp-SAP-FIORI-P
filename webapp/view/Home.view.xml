<mvc:View
    controllerName="com.bootcamp.sapui5.freestylee.controller.Home"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:c="sap.ui.core"
>
    <Page
        id="page"
        title="{i18n>title}"
    >
        <VBox
            id="vb"
            class="sapUiSmallMargin"
        >
            <Text
                id="text1"
                text="{i18n>textoi18n}"
            />
            <Toolbar id="toolbarID" >
            <Input id="inputID" change="onChange"/>
            <Button
                id="btnBuscar"
                text="{i18n>textBuscar}"
                
                press="onPress"
            />
            </Toolbar>
        </VBox>

        <VBox id="vTest2">
            <c:Fragment
                fragmentName="com.bootcamp.sapui5.freestylee.view.fragments.ProductsTable"
                type="XML"
            />
        </VBox>
    </Page>
</mvc:View>
